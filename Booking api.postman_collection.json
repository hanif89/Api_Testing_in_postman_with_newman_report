							"\r",
							"pm.test(\"Total Price Check \",function(){\r",
							"    pm.expect(pm.environment.get(\"tootalprice\")).to.eql(responseData.totalprice.toString())\r",
							"})\r",
							"\r",
							"pm.test(\"Deposit Paid Check \",function(){\r",
							"    pm.expect(pm.environment.get(\"dpaid\")).to.eql(responseData.depositpaid.toString())\r",
							"})\r",
							"\r",
							"pm.test(\"Check in Date \",function(){\r",
							"    pm.expect(pm.environment.get(\"check_in_date\")).to.eql(responseData.bookingdates.checkin)\r",
							"})\r",
							"\r",
							"pm.test(\"Check out Date \",function(){\r",
							"    pm.expect(pm.environment.get(\"check_out_date\")).to.eql(responseData.bookingdates.checkout)\r",
							"})\r",
							"}\r",
							"else if(status_code==404){\r",
							"    pm.test(\"ID expired\")\r",
